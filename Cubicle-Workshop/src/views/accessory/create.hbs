<main>
    <h1>Create Accessory</h1>
    <div class="form">
        <form method="POST">
            <label for="name">Name</label>
            <input type="text" id="name" name="name" value="{{accessoryData.name}}">
            <label for="name">Description</label>
            <textarea id="description" name="description">{{accessoryData.description}}</textarea>
            <label for="image">ImageUrl</label>
            <input type="text" id="image" name="imageUrl" value="{{accessoryData.imageUrl}}">
            <input type="submit" value="Create">
        </form>
    </div>
</main>